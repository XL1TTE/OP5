<Application x:Class="OP5.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vms="clr-namespace:OP5.MVVM.ViewModel"
             xmlns:view="clr-namespace:OP5.MVVM.View"
             xmlns:local="clr-namespace:OP5">
    <Application.Resources>
        <ResourceDictionary>

            <DataTemplate DataType="{x:Type vms:MainWindowViewModel}">
                <view:MainWindow />
            </DataTemplate>
            <DataTemplate DataType="{x:Type vms:TableViewModel}">
                <view:TableView />
            </DataTemplate>
            
        </ResourceDictionary>
    </Application.Resources>
</Application>
